{
  "name": "whats_for_dinner",
  "version": "1.0.0",
  "type": "module",
  "imports": {
    "#*": "./*.js",
    "#db/*": "./db/*"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "start": "node server.js",
    "dev:server": "node --watch --env-file=.env server.js",
    "test": "vitest",
    "db:schema": "psql -d whats_for_dinner -f src-backend/db/schema.sql",
    "db:seed": "node --env-file=.env src-backend/dbSeed/seed.js",
    "db:reset": "npm run db:schema && npm run db:seed"
  },
  "engines": {
    "node": ">=22.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@vitejs/plugin-react-swc": "^4.2.1",
    "eslint": "^9.39.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "supertest": "^7.1.4",
    "vite": "^7.2.2",
    "vitest": "^4.0.8"
  },
  "dependencies": {
    "@faker-js/faker": "^10.1.0",
    "bcrypt": "^6.0.0",
    "cloudinary": "^2.8.0",
    "dotenv": "^17.2.3",
    "express": "^4.21.2",
    "express-fileupload": "^1.5.2",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.16.3",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-icons": "^5.5.0",
    "react-rating-stars-component": "^2.2.0",
    "react-router": "^7.9.5",
    "serve": "^14.2.5"
  }
}
